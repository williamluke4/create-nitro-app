{
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    ["styled-components", 
      { 
        "ssr": true, 
        "displayName": true, 
        "preprocess": false 
      } 
    ],
    "transform-flow-strip-types"
  ],
  "presets": [
    "next/babel",
    "flow"
  ],
  "ignore": []
}
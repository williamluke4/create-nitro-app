
{
  "parser": "babel-eslint",
  "extends": ["airbnb","plugin:flowtype/recommended"],
  "globals": {
    "document": true,
    "window": true,
    "process": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "linebreak-style": 0
  }
}